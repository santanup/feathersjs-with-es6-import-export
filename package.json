{
  "name": "feathersjs-with-es-6-import-export",
  "description": "Use es6 import/export syntax in feathersjs by using babeljs",
  "version": "1.0.0",
  "homepage": "https://github.com/santanup/feathersjs-with-es6-import-export",
  "private": false,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Santanu Prasad Sahoo",
    "email": "sahoosantanu92@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "cjstoes6": "cjs-to-es6 --verbose src",
